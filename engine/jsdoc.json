{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./src/"],
        "exclude": ["./src/build"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./docs/",
        "template": "./templates/default",
        "recurse": true,
        "private": false
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}